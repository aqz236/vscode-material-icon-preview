{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "bun run generate-meta && bun run copy-icons && vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "copy-icons": "rm -rf public/icons && cp -r node_modules/material-icon-theme/icons public/icons",
    "generate-meta": "bun run scripts/generate-icon-meta"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "@radix-ui/react-dialog": "^1.1.15",
