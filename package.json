    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}